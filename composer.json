    "laravel/framework": "^10.0 || ^9.0 || >=8.40.0 || ^7.0",
    "ext-curl": "*"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "LLPDDone",
      "email": "llpddone@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
